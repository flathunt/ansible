pipeline {
    agent any

    options {
        ansiColor('xterm')
    }

    stages {
        stage('Clone repo') {
            steps {
                git branch: 'main', credentialsId: 'marcusc', url: 'git@github.com:flathunt/ansible.git'
            }
        }    
        stage('Run nmap') {
            steps {
	        sh '''
		    sudo nmap -sn 192.168.68.0/24
                '''
            }
        }
    }
}
